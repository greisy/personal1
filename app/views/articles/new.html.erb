<h1>New article</h1>
	<%= form_for :article, url: articles_path do |f| %> <!--form_for es un form builder. Se coloca <%=%> para indicar que se va a colocar lenguaje rails  dentro del template html-->
		<p>
			<%= f.label :title %>
			<%= f.text_field :title%>
		</p> 
		<p>
			<%= f.label :text %><br>
			<%= f.text_area :text%>
		</p>
		
		<p>
			<%= f.submit%>
		</p>
	<% end %>
